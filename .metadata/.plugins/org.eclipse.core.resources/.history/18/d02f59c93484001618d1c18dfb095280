package Logica;

import javax.swing.JLabel;
import javax.swing.JPanel;

public class Componente {
   protected JLabel label;
   
   public Componente(){
	   
   }
   
   public void insertar(JPanel panelMapa){
	   panelMapa.add(label);
   }
   public void borrar(JPanel panelMapa){
	   panelMapa.remove(label);
	   panelMapa.revalidate();
	   panelMapa.repaint();
   }
}
