package Obstaculos;




import javax.swing.Icon;
import javax.swing.ImageIcon;

import Visitor.*;



public class Ladrillo extends Obstaculo{	
	
	 public Ladrillo(){
		 super();
		 
		 E = new elementoLadrillo();
		 
		 graf = new ImageIcon(getClass().getResource("/Iconos/Ladrillo.png"));

	 }	 
	 
	 
	
	public String getNombre(){
		return "ladrillo";
	}
	public Icon getIcon(){
		
		return graf;
	 }




}
